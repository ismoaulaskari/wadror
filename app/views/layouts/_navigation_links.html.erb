<%# <add navigation links to this file %>
<li><%= link_to 'Breweries', breweries_path %></li>
<li><%= link_to 'Beers', beers_path %></li>
<li><%= link_to 'Ratings', ratings_path %></li>
<li><%= link_to 'Users', users_path %></li>
<li><%= link_to 'Beer clubs', beer_clubs_path %></li>
<li><%= link_to 'Bar search', places_path %></li>
<% if not current_user.nil? %>
  <li>Signed in as <%= link_to current_user.username, user_path(current_user.id) %></li>
  <li><%= link_to 'Signout', signout_path, method: :delete %></li>
  <li><%= link_to 'Rate a beer', new_rating_path %></li>
  <li><%= link_to 'Join a beer club', new_membership_path %></li>
<% else %>
  <li><%= link_to 'Signin', signin_path %></li>
  <li><%= link_to 'Signup', signup_path %></li>
<% end %>
<% if session[:last_rating].nil? %>
  <li>no ratings given</li>
<% else %>
  <li><i> previous rating: <%= session[:last_rating] %> </i></li>
<% end %>

